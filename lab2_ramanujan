#include <math.h>
#include <stdio.h>

#define c1 (2*sqrt(2)/9801)

float fatorial(int fat){
    float z=1;
    for(float x=1;x<fat;x++){
        z = z*(x+1);
    }
    return z;
}
float den(int k){
    float denominador;
    denominador = pow(fatorial(k),4)*pow(396,(4*k));
    return denominador;
}
float soma(int k){
    int z;
    for(float x=0;x<k;x++){
    z += (fatorial(4*x))*((1103+(26390*x))/den(x));

    }
    return z;
}
int main(){
    int k;
    float inv;
    float pi;
    printf("Digite valor de k");
    scanf("%d", &k);
    inv = c1*soma(k);
    pi = 1/inv;
    printf("%f", pi);
}
